<html>
<head>
</head>

<body>
<div class="dashboard summary-view" id="Probe_Monitor">

    <div class="row">
        <div class="col-xs-4 app-box default-height app-colum right-block">

            <div class="sysinfo-footer">
                <div class="col-xs-12">
                    <span class="title">用户</span>
                </div>
                <div class="text-right link-container">
                    <div>
                        <a type="button" class="refresh" id="refresh_client">
                            <i class="refresh-icon icon"></i>
                        </a>
                        <a type="button" class="link-detail" id="client_details" title="更多" href="clientprobe.clients">
                            <i class="detail-icon icon"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div>


                <div class="sysinfo-body">
                    <div class="col-xs-6">
                        <div class="myEchart" id="Assco_client"></div>
                    </div>
                    <div class="col-xs-6">
                        <div class="myEchart" id="Active_pie"></div>
                    </div>
                </div>

                <div class="col-xs-2 text-center"  style="color:gray;border-right-style:solid;border-width:1px;border-color:#e5e8e8;">总计</div>
                <div class="col-xs-2 text-center" style="color:gray">关联</div>
                <div class="col-xs-3 text-center" style="color:gray;border-right-style:solid;border-width:1px;border-color:#e5e8e8;">非关联</div>
                <div class="col-xs-2 text-center" style="color:gray">活跃</div>
                <div class="col-xs-3 text-center" style="color:gray">非活跃</div>
                <!-- <hr/> -->
                <div id = "client_block">
                    <div class="col-xs-2 text-center"  style="color:black;border-right-style:solid;border-width:1px;border-color:#e5e8e8;">
                        <span id="Total_Total" style="color:black"></span>
                    </div>
                    <div class="col-xs-2 text-center" >
                        <span id="Total_associated" style="color:black"></span>
                    </div>
                    <div class="col-xs-3 text-center" style="color:black;border-right-style:solid;border-width:1px;border-color:#e5e8e8;">
                        <span id="Total_dissociated" style="color:black"></span>
                    </div>
                </div>
                <div id = "Status_block">
                    <div class="col-xs-2 text-center">
                        <span id="Total_Active" style="color:black"></span>
                    </div>
                    <div class="col-xs-3 text-center">
                        <span id="Total_Inactive" style="color:black"></span>
                    </div>
                </div>
            </div>
        </div>


        <div class="col-xs-4 app-colum right-block">
            <div class="app-box default-height">
                <div class="sysinfo-footer">
                    <div class="col-xs-12">
                        <span class="title">停留时间</span>
                    </div>
                    <div class="text-right link-container">
                        <div>
                            <a type="button" class="refresh" id="refresh_rssi">
                                <i class="refresh-icon icon"></i>
                            </a>
                            <!--                             <a type="button" class="link-detail" id="client_details" href="clientprobe.clients">
                                <i class="detail-icon icon"></i>
                            </a> -->
                        </div>
                    </div>
                </div>
                <div class="sysinfo-body">
                    <div class="col-xs-12">
                        <div class="myEchart" id="Rssi_Pie"></div>
                    </div>

                </div>

            </div>
        </div>

        <div class="col-xs-4 app-colum right-block">
            <div class="app-box default-height">
                <div class="sysinfo-footer">
                    <div class="col-xs-12">
                        <span class="title">设备厂商</span>
                    </div>
                    <div class="text-right link-container">
                        <div>
                            <a type="button" class="refresh" id="refresh_vender">
                                <i class="refresh-icon icon"></i>
                            </a>
                            <!--                             <a type="button" class="link-detail" id="client_details" href="clientprobe.clients">
                                <i class="detail-icon icon"></i>
                            </a> -->
                        </div>
                    </div>
                </div>
                <div class="sysinfo-body">

                    <div class="col-xs-12">
                        <div class=" myEchart" id="Vender_pie"></div>
                    </div>

                </div>

            </div>
        </div>

        <div class="col-xs-12 app-colum">
            <div class="app-box default-height">
                <div class="sysinfo-footer">
                    <div class="col-xs-12">
                        <span class="title">探针统计</span>
                    </div>
                    <div class="text-right link-container">
                        <div>
                            <a type="button" class="refresh" id="refresh_Probe">
                                <i class="refresh-icon icon"></i>
                            </a>
                            <a type="button" class="filter" id="filter">
                                <i class="filter-icon icon"></i>
                            </a>
                            <!--                             <a type="button" class="link-detail" id="client_detail" href="clientprobe.clients">
                                <i class="detail-icon icon"></i>
                            </a> -->
                        </div>
                        <div class="hide" id="filter_time">
                            <div class="filter-container">
                                <span>时间间隔</span>
                                <select class="singleSelect big" id="Interval"> </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="sysinfo-body">
                    <div id="probe_line" class="myEchart col-xs-12"></div>
                </div>

            </div>
        </div>
    </div>
</div>

<div id="probe_statistics_rc" class="rc-define"
     TYPE="总计,信号强(0~39dBi),信号中(40~70dBi),信号弱(71~100dBi)"
     NAME="关联的,活跃的"
     INTERVAL="1小时,2小时,3小时,4小时,5小时,6小时,7小时,8小时,9小时,10小时,11小时,12小时,13小时,14小时,15小时,16小时,17小时,18小时,19小时,20小时,21小时,22小时,23小时,24小时"
     VENDER="苹果,HTC,魅族,小米,三星,其他"
     CLIENTTIME="<15分,15~30分,30~60分,1~2小时,>2小时"
     AXIS="人数,时间"
     DATA=<%=JSON.stringify(statistics)%>

        ></div>

</body>
</html>
<script language="javascript" type="text/javascript">
    Utils.Pages.loadJS("clientprobe.statistics");
</script>